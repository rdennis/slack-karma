    "url": "https://github.com/rdennis/slack-karma/issues"
  },
  "homepage": "https://github.com/rdennis/slack-karma#readme",
  "dependencies": {
    "@slack/client": "^4.8.0",
    "@slack/events-api": "^2.1.1",
    "body-parser": "^1.20.1",
    "dotenv": "^6.1.0",
    "express": "^4.18.2",
    "newrelic": "^5.10.0",
    "pg": "^7.6.0"
  },
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/dotenv": "^4.0.3",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.5",
